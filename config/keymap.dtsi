/ {
    keymap {
        compatible = "zmk,keymap";

        base_layer {
            bindings = <
                &kp ESC    &kp Q     &kp W     &kp E     &kp R     &kp T     &kp Y     &kp U     &kp I     &kp O
                &kp TAB    &kp A     &kp S     &kp D     &kp F     &kp G     &kp H     &kp J     &kp K     &kp L
                &kp LSFT   &kp Z     &kp X     &kp C     &kp V     &kp B     &kp N     &kp M     &kp COMMA &kp DOT
                &kp LCTRL  &kp LALT  &kp SPACE &kp RALT  &kp RCTRL &kp LEFT  &kp DOWN  &kp UP    &kp RIGHT &kp ENTER
                &kp CAPS   &kp GRAVE &kp MINUS &kp EQUAL &kp LBKT  &kp RBKT  &kp BSLS  &kp SEMI  &kp QUOTE &kp DEL
                &encoder0_cw &encoder0_ccw &encoder0_press
                &encoder1_cw &encoder1_ccw &encoder1_press
            >;
        };
    };
};

